{
  "name": "@folhadeadao/backend",
  "version": "2.0.0",
  "description": "Funil de Vendas - Backend",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "lb-tsc -p ./tsconfig.json",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist *.tsbuildinfo",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t folhadeadao/backend .",
    "docker:run": "docker run -p 5000:5000 -d folhadeadao/backend",
    "migrate": "node ./dist/migrate",
    "prestart": "npm run build",
    "debug": "node --inspect-brk -r source-map-support/register .",
    "start": "node -r source-map-support/register .",
    "prepublishOnly": "npm run test"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^4.2.9",
    "@loopback/authorization": "^0.6.2",
    "@loopback/boot": "^2.3.5",
    "@loopback/context": "^3.9.2",
    "@loopback/core": "^2.9.1",
    "@loopback/openapi-v3": "^3.4.5",
    "@loopback/repository": "^2.9.0",
    "@loopback/rest": "^5.2.0",
    "@loopback/rest-explorer": "^2.2.6",
    "@loopback/security": "^0.2.14",
    "@loopback/service-proxy": "^2.3.4",
    "jsonwebtoken": "^8.5.1",
    "loopback-connector-mysql": "^5.4.4",
    "loopback-connector-postgresql": "^5.0.1",
    "multer": "^1.4.2",
    "promisify": "0.0.3",
    "speakeasy": "^2.0.0",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^6.1.0",
    "@loopback/eslint-config": "^8.0.3",
    "@loopback/testlab": "^3.2.0",
    "@types/multer": "^1.4.3",
    "@types/node": "^10.17.26",
    "@types/speakeasy": "^2.0.5",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-eslint-plugin": "^2.2.1",
    "eslint-plugin-mocha": "^7.0.0",
    "source-map-support": "^0.5.19",
    "typescript": "~3.9.5"
  }
}
